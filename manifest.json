{
  "manifest_version": 3,
  "name": "115ToAlist",
  "description": "Update 115 cookies to Alist automatically.",
  "version": "1.0",
  "permissions": [
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
   "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}